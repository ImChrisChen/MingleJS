{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "modules": false,
                "targets": {
                    "browsers": "last 2 versions, not ie <= 9"
                }
            },
            {
                "module": false
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": false
            }
        ]
    ]
}
