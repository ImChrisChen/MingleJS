    "scripts": {
        "build": "npm run rm-dist && NODE_ENV=production webpack --config webpack.config.js --progress --color && npm run update-version",
        "deploy": "npm run rm-dist && webpack --config webpack.config.js --progress --color && npm run update-version && git add . && git commit -m 'deploy' && git push all",
        "start-all": "pm2 start ecosystem.config.js && pm2 start ./server/ecosystem.config.js",
        "pre": "npm run rm-dist NODE_ENV=pre webpack && npm run update-version",
        "dev": "NODE_ENV=development webpack-dev-server --disableHostCheck=true --hot --inline --progress --profile --colors --config webpack.config.js",
        "rm-dist": "rm -rf ./dist",
        "update-version": "node ./script/script.js"
    },
